						<tgroup block-style="1" cols="8" grid-typeface="1.1" rowsep="0" thead-tbody-ldg-size="0.7.9"><colspec coldef="txt-no-ldr-no-spread" colname="column1" colwidth="8pts" min-data-value="8"></colspec><colspec coldef="txt-no-ldr-no-spread" colname="column2" colwidth="35pts" min-data-value="35"></colspec><colspec coldef="txt" colname="column3" colwidth="86pts" min-data-value="60"></colspec><colspec coldef="txt-no-ldr-no-spread" colname="column4" colwidth="25pts" min-data-value="25"></colspec><colspec coldef="txt-no-ldr-no-spread" colname="column5" colwidth="38pts" min-data-value="38"></colspec><colspec coldef="txt-no-ldr-no-spread" colname="column6" colsep="1" colwidth="38pts" min-data-value="38"></colspec><colspec coldef="txt-no-spread" colname="column7" colwidth="38pts" min-data-value="38"></colspec><colspec align="justify" coldef="fig" colname="column8" colwidth="12pts" min-data-value="3"></colspec>
							<tbody>
								<row><entry align="left" colname="column1" leader-modify="clr-ldr" rowsep="0" stub-definition="txt-clr" stub-hierarchy="1"></entry><entry align="left" colname="column2" leader-modify="clr-ldr" rowsep="0" valign="top">9902.63.07</entry><entry align="left" colname="column3" leader-modify="force-ldr-bottom" rowsep="0" valign="bottom">Man-made shells used in the manufacture of sleeping bags
						(provided for in subheading 6307.90.89)</entry><entry align="left" colname="column4" leader-modify="clr-ldr" rowsep="0" valign="bottom">Free</entry><entry align="left" colname="column5" leader-modify="clr-ldr" rowsep="0" valign="bottom">No
